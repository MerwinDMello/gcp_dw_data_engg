export AC_EXP_SQL_STATEMENT="SELECT 'CBS_STG_DEPARTMENT_SSD' || ',' || CAST(Count(*) AS VARCHAR(20)) || ',' || CAST(SUM(CHARACTER_LENGTH(Trim(DPT.Short_Dept_Name))) AS VARCHAR(20)) || ',' as SOURCE_STRING FROM EDWFS_Base_Views.Department DPT WHERE DPT.Company_Code = 'H' AND (DPT.Short_Dept_Name IS NOT NULL AND LTRIM(RTRIM(DPT.Short_Dept_Name)) <> '' AND CHAR2HEXINT(DPT.Short_Dept_Name) <> '00000000000000000000') AND (DATE BETWEEN DPT.Eff_From_Date AND DPT.Eff_To_Date)"
export JOBNAME='CBS_STG_DEPARTMENT_SSD'
export AC_EXP_TOLERANCE_PERCENT=0
export AC_EXP_TOLERANCE_AMT=0
export AC_ACT_SQL_STATEMENT="SELECT 'CBS_STG_DEPARTMENT_SSD' + ',' + CAST(Count(*) AS VARCHAR(20)) + ',' + CAST(SUM(LEN(DPT.Department_Description)) AS VARCHAR(20)) + ',' as SOURCE_STRING FROM dbo.Stg_Department DPT"