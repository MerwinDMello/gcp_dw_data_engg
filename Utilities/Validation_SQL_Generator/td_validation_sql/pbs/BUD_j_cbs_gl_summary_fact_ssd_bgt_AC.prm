export AC_EXP_SQL_STATEMENT="SELECT 'CBS_GL_SUMMARY_FACT_SSD_BGT' + ',' + CAST(Count(*) AS VARCHAR(20)) + ',' + CAST(SUM(LEN(GLS.Scenario_Sid)) AS VARCHAR(20)) + ',' + CAST(SUM(GLS.Amount) AS VARCHAR(20)) + ',' as SOURCE_STRING FROM dbo.vw_GL_Summary_Fact_SSD_Budget GLS"
export JOBNAME='CBS_GL_SUMMARY_FACT_SSD_BGT'
export AC_EXP_TOLERANCE_PERCENT=0
export AC_EXP_TOLERANCE_AMT=0
export AC_ACT_SQL_STATEMENT="SELECT 'CBS_GL_SUMMARY_FACT_SSD_BGT' + ',' + CAST(Count(*) AS VARCHAR(20)) + ',' + CAST(SUM(LEN(SCN.Scenario_Sid)) AS VARCHAR(20)) + ',' + CAST(SUM(FCT.Amount) AS VARCHAR(20)) + ',' as SOURCE_STRING FROM (SELECT FCT.Scenario, FCT.Amount FROM dbo.vw_Fact_SSD_Budget_Member_Names FCT WHERE FCT.Scenario IN ('Actual') AND FCT.Account NOT IN (SELECT ACT.Account_Name_Child FROM dbo.Dim_Account_SSD_Budget ACT WHERE ACT.Account_Name_Child IN (SELECT ACT.Account_Name_Child FROM dbo.Ref_Account_Static_Members ACT)) AND FCT.Employee = 'No_Employee' AND FCT.Value = 'Input' UNION ALL SELECT FCT.Scenario, FCT.Amount FROM dbo.vw_Fact_SSD_Budget_Member_Names FCT WHERE FCT.Scenario IN ('Current_Budget') AND FCT.Account NOT IN (SELECT ACT.Account_Name_Child FROM dbo.Dim_Account_SSD_Budget ACT WHERE ACT.Account_Name_Child IN (SELECT ACT.Account_Name_Child FROM dbo.Ref_Account_Static_Members ACT)) AND FCT.Employee = 'No_Employee' AND FCT.Value = 'Input' AND FCT.Year IN (SELECT YER.Year_Name_Child FROM dbo.Dim_Year_SSD_Budget YER WHERE ISNUMERIC(RIGHT(Year_Name_Child, 4)) = 1 AND CAST(RIGHT(YER.Year_Name_Child, 4) AS SMALLINT) < (SELECT  CAST (Keyvalue as INT) AS Year FROM dbo.Ref_Global_Variables AS BGT WHERE Keyword = 'GL_Summary_Rollover_Year'))) FCT INNER JOIN dbo.Dim_Scenario_SSD_Budget SCN ON FCT.Scenario = SCN.Scenario_Name_Child"
