export AC_EXP_SQL_STATEMENT="WITH RECURSIVE Account_Hierarchy (FA_Sub_Acct_Msr_Sid, ASO_BSO_Storage_Code, FA_Sub_Acct_Name_Parent, FA_Sub_Acct_Name_Child, FA_Sub_Acct_Alias_Name, Alias_Table_Name, Sort_Key_Num, Consolidation_Code, Storage_Code, Two_Pass_Calc_Code, Formula_Text, Member_Solve_Order_Num, Variance_Reporting_Code, Time_Balance_Code, Skip_Missing_Code, Production_Ind, Financial_Analytics_Ind, ASD_Ind, OSG_Msr_Ind, Account_Msr_Ind, Budget_Msr_Ind, Essbase_Comment_Text, Essbase_UDA, FA_Sub_Acct_Hier_Name, FS_Sub_Acct_Name_Child2) AS (SELECT ACT.FA_Sub_Acct_Msr_Sid, ACT.ASO_BSO_Storage_Code, ACT.FA_Sub_Acct_Name_Parent, ACT.FA_Sub_Acct_Name_Child, ACT.FA_Sub_Acct_Alias_Name, ACT.Alias_Table_Name, ACT.Sort_Key_Num, ACT.Consolidation_Code, ACT.Storage_Code, ACT.Two_Pass_Calc_Code, ACT.Formula_Text, ACT.Member_Solve_Order_Num, ACT.Variance_Reporting_Code, ACT.Time_Balance_Code, ACT.Skip_Missing_Code, ACT.Production_Ind, ACT.Financial_Analytics_Ind, ACT.ASD_Ind, ACT.OSG_Msr_Ind, ACT.Account_Msr_Ind, ACT.Budget_Msr_Ind, ACT.Essbase_Comment_Text, ACT.Essbase_UDA, ACT.FA_Sub_Acct_Hier_Name, ACT.FA_Sub_Acct_Name_Child FROM EDWFS_Base_Views.Dim_FA_Sub_Acct_Measure ACT WHERE ACT.FA_Sub_Acct_Hier_Name = 'Fin_Measures' AND ACT.FA_Sub_Acct_Name_Child = 'F90340' UNION ALL SELECT ACT.FA_Sub_Acct_Msr_Sid, ACT.ASO_BSO_Storage_Code, ACT.FA_Sub_Acct_Name_Parent, ACT.FA_Sub_Acct_Name_Child, ACT.FA_Sub_Acct_Alias_Name, ACT.Alias_Table_Name, ACT.Sort_Key_Num, ACT.Consolidation_Code, ACT.Storage_Code, ACT.Two_Pass_Calc_Code, ACT.Formula_Text, ACT.Member_Solve_Order_Num, ACT.Variance_Reporting_Code, ACT.Time_Balance_Code, ACT.Skip_Missing_Code, ACT.Production_Ind, ACT.Financial_Analytics_Ind, ACT.ASD_Ind, ACT.OSG_Msr_Ind, ACT.Account_Msr_Ind, ACT.Budget_Msr_Ind, ACT.Essbase_Comment_Text, ACT.Essbase_UDA, ACT.FA_Sub_Acct_Hier_Name, ACT.FA_Sub_Acct_Name_Child FROM EDWFS_Base_Views.Dim_FA_Sub_Acct_Measure ACT INNER JOIN Account_Hierarchy HRC ON ACT.FA_Sub_Acct_Name_Parent = HRC.FA_Sub_Acct_Name_Child WHERE ACT.FA_Sub_Acct_Hier_Name = 'Fin_Measures') SELECT 'CBS_STG_ACCOUNT_SSD' || ',' || CAST(Count(*) AS VARCHAR(20)) || ',' || CAST(SUM(CHARACTER_LENGTH(TRIM(ACT.FA_Sub_Acct_Name_Child))) AS VARCHAR(20)) || ',' || CAST(SUM(ACT.Member_Solve_Order_Num) AS VARCHAR(20)) || ',' as SOURCE_STRING FROM Account_Hierarchy ACT"
export JOBNAME='CBS_STG_ACCOUNT_SSD'
export AC_EXP_TOLERANCE_PERCENT=0
