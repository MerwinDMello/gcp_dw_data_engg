export AC_EXP_SQL_STATEMENT="SELECT 'CBS_STG_EMPLOYEE_SSD' || ',' || CAST(Count(*) AS VARCHAR(20))|| ',' || CAST(SUM(LENGTH(LTrim(RTrim(EMP.Exempt_EMP)))) AS VARCHAR(20)) || ',' || CAST(SUM(EMP.Monthly_FTE)AS VARCHAR(20)) || ',' as SOURCE_STRING FROM LPAHCA.ZVCLRTYBUD EMP WHERE (EMP.Company = 5910 OR (EMP.Company = 5950 AND LTRIM(RTRIM(EMP.Process_Level)) <> '10207'))"
export JOBNAME='CBS_STG_EMPLOYEE_SSD'
export AC_EXP_TOLERANCE_PERCENT=0
export AC_EXP_TOLERANCE_AMT=0
export AC_ACT_SQL_STATEMENT="SELECT 'CBS_STG_EMPLOYEE_SSD' + ',' + CAST(Count(*) AS VARCHAR(20)) + ',' + CAST(SUM(LEN(EMP.Is_Exempt)) AS VARCHAR(20)) + ',' + CAST(SUM(EMP.FTE) AS VARCHAR(20)) + ',' as SOURCE_STRING FROM dbo.Stg_Employee EMP"
export DB2_OWNER=LPAHCA
