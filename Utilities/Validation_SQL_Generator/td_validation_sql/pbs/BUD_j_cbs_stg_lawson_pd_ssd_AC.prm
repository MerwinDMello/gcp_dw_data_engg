export AC_EXP_SQL_STATEMENT="SELECT 'CBS_STG_LAWSON_PD_SSD' || ',' || CAST(COUNT(HCA_COID) AS VARCHAR(20)) || ',' || CAST(SUM(LENGTH(LTRIM(RTRIM(DESCRIPTION)))) AS VARCHAR(20)) || ',' CONCAT CAST(SUM(R_HOURS) as VARCHAR(20)) || ',' as SOURCE_STRING FROM LPAHCA.ZNBUDHOURS, SYSIBM.SYSDUMMY1 WHERE R_YEAR >= (YEAR(CURRENT DATE) - 2)"
export JOBNAME='CBS_STG_LAWSON_PD_SSD'
export AC_EXP_TOLERANCE_PERCENT=0
export AC_EXP_TOLERANCE_AMT=0
export AC_ACT_SQL_STATEMENT="SELECT 'CBS_STG_LAWSON_PD_SSD' + ',' + CAST(COUNT(COID) AS VARCHAR(20)) + ',' + CAST(SUM(LEN(DESCRIPTION)) AS VARCHAR(20))+ ',' + CAST(SUM(R_HOURS)AS VARCHAR(20)) + ',' as SOURCE_STRING from dbo.Stg_Lawson_Pd"
